{
	"extends": "brightspace/polymer-3-config",
	"env": {
    "es6": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
      "sourceType": "module"
  },
  "rules": {
    "no-var": "error",
    "prefer-const": "error",
    "sort-class-members/sort-class-members": "off"
  },
  "globals": {
    "fastdom": false
  }
}
